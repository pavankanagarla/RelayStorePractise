{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.1",
    "react-loadable": "^5.5.0",
    "react-relay": "^5.0.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "test": "jest",
    "dev": "webpack-server",
    "build": "webpack --config webpack.prod.js",
    "postbuild": "copy .\\public\\index.html .\\dist\\index.html",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "lint": "eslint src",
    "flow": "flow",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|scss|graphql)\"",
    "format": "npm run prettier -- --write ",
    "validate": "npm run lint && npm run test && npm run format && npm run flow",
    "precommit": "lint-staged && npm run flow",
    "relay": "relay-compiler --watchman false --src ./src --schema ./schema.graphql"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.7",
    "babel-plugin-relay": "^3.0.0",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.94.0",
    "graphql": "^14.1.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-junit": "^6.3.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "relay-compiler": "^3.0.0",
    "sass-loader": "^7.1.0",
    "serve": "^10.1.2",
    "style-loader": "^0.23.1",
    "svg-loader": "^0.0.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-serve": "^2.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
